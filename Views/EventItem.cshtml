@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.EventItem>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "Master.cshtml";
}


<!-- Jumbotron, w title -->
<div class="jumbotron text-center jumbotron-fluid">
	<div class="container">
		<h1 class="display-1">@Model.Value("eventTitle")</h1>
	</div>
</div>

<!-- Main container -->

<!-- Blogpost content -->
<div class="container">

	<div class="row section">
		<div class="col-md-12">
			@Model.Value("eventContent")
		</div>
	</div>
</div>